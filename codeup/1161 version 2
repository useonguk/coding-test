#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,sum = 0;

    scanf("%d %d",&a,&b);

    sum = a + b;

    if((a % 2 == 0)) { printf("짝수+"); }
    else { printf("홀수+"); }

    if((b % 2 == 0)) { printf("짝수"); }
    else { printf("홀수"); }

    if((sum % 2 == 0)) { printf("=짝수"); }
    else { printf("=홀수"); }

    return 0;
}
